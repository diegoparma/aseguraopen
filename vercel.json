{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "framework": "other",
  "regions": ["us-east-1"],
  "functions": {
    "api/**: {
      "runtime": "python3.12"
    }
  },
  "env": {
    "OPENAI_API_KEY": "@OPENAI_API_KEY",
    "TURSO_DATABASE_URL": "@TURSO_DATABASE_URL",
    "TURSO_AUTH_TOKEN": "@TURSO_AUTH_TOKEN"
  },
  "public": false
}
